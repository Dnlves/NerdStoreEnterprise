{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (Cliente)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-cliente-api",
      "program": "${workspaceFolder}/src/services/NSE.Clientes.API/bin/Debug/net5.0/NSE.Clientes.API",
      "args": [],
      "cwd": "${workspaceFolder}/src/services/NSE.Clientes.API",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Launch (Catalogo)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-catalogo-api",
      "program": "${workspaceFolder}/src/services/NSE.Catalogo.API/bin/Debug/net5.0/NSE.Catalogo.API",
      "args": [],
      "cwd": "${workspaceFolder}/src/services/NSE.Catalogo.API",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Launch (MVC)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-mvc",
      "program": "${workspaceFolder}/src/web/NSE.WebApp.MVC/bin/Debug/net5.0/NSE.WebApp.MVC.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/web/NSE.WebApp.MVC",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Launch (Identidade)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-identidade-api",
      "program": "${workspaceFolder}/src/services/NSE.Identidade.API/bin/Debug/net5.0/NSE.Identidade.API.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/services/NSE.Identidade.API",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ],
  "compounds": [
    {
      "name": "Identidade + MVC + Catalogo + Cliente",
      "configurations": [
        ".NET Core Launch (MVC)",
        ".NET Core Launch (Identidade)",
        ".NET Core Launch (Catalogo)",
        ".NET Core Launch (Cliente)"
      ]
    }
  ]
}